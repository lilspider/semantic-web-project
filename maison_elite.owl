<?xml version="1.0"?>
<rdf:RDF xmlns="http://maison-elite.org/ontology#"
     xml:base="http://maison-elite.org/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:me="http://maison-elite.org/ontology#">

    <owl:Ontology rdf:about="http://maison-elite.org/ontology">
        <rdfs:label>Maison Élite Restaurant Ontology</rdfs:label>
        <rdfs:comment>A fine-dining restaurant ontology covering dishes, chefs, menus, ingredients, reservations, customers, and awards.</rdfs:comment>
    </owl:Ontology>


    <!-- ═══════════════════════════════════════════════════════════
         OBJECT PROPERTIES
         ═══════════════════════════════════════════════════════════ -->

    <!-- Restaurant employs Chef -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#employs">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Chef"/>
    </owl:ObjectProperty>

    <!-- Restaurant hasMenu Menu -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#hasMenu">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Menu"/>
    </owl:ObjectProperty>

    <!-- Restaurant receivedAward Award -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#receivedAward">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Award"/>
    </owl:ObjectProperty>

    <!-- Menu includes Dish -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#includes">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Menu"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Dish"/>
    </owl:ObjectProperty>

    <!-- Dish preparedBy Chef -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#preparedBy">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Chef"/>
    </owl:ObjectProperty>

    <!-- Dish containsIngredient Ingredient -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#containsIngredient">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Ingredient"/>
    </owl:ObjectProperty>

    <!-- Customer hasReservation Reservation -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#hasReservation">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Customer"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Reservation"/>
    </owl:ObjectProperty>

    <!-- Reservation atRestaurant Restaurant -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#atRestaurant">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Reservation"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
    </owl:ObjectProperty>

    <!-- Chef earnedAward Award -->
    <owl:ObjectProperty rdf:about="http://maison-elite.org/ontology#earnedAward">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Chef"/>
        <rdfs:range  rdf:resource="http://maison-elite.org/ontology#Award"/>
    </owl:ObjectProperty>

    <!-- Chef specialisesIn Cuisine (string-valued, modelled as annotation) -->


    <!-- ═══════════════════════════════════════════════════════════
         DATA PROPERTIES
         ═══════════════════════════════════════════════════════════ -->

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#name">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#rating">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#price">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#location">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#cuisine">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Chef"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#capacity">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Restaurant"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#yearsExperience">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Chef"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#specialty">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Chef"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#nationality">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Chef"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#isVegan">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#allergens">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#calories">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Dish"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#reservationDate">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Reservation"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#partySize">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Reservation"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#tableNumber">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Reservation"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#confirmed">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Reservation"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#memberSince">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Customer"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#totalVisits">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Customer"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#origin">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Ingredient"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#seasonal">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Ingredient"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#year">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Award"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://maison-elite.org/ontology#issuedBy">
        <rdfs:domain rdf:resource="http://maison-elite.org/ontology#Award"/>
        <rdfs:range  rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>


    <!-- ═══════════════════════════════════════════════════════════
         CLASSES
         ═══════════════════════════════════════════════════════════ -->

    <owl:Class rdf:about="http://maison-elite.org/ontology#Restaurant"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#Menu"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#Ingredient"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#Award"/>

    <!-- Chef hierarchy -->
    <owl:Class rdf:about="http://maison-elite.org/ontology#Chef"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#HeadChef">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Chef"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#SousChef">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Chef"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#PastryChef">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Chef"/>
    </owl:Class>

    <!-- Customer hierarchy -->
    <owl:Class rdf:about="http://maison-elite.org/ontology#Customer"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#VIPCustomer">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Customer"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#RegularCustomer">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Customer"/>
    </owl:Class>

    <!-- Dish hierarchy -->
    <owl:Class rdf:about="http://maison-elite.org/ontology#Dish"/>
    <owl:Class rdf:about="http://maison-elite.org/ontology#Starter">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Dish"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#MainCourse">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Dish"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#Dessert">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Dish"/>
    </owl:Class>

    <!-- Menu hierarchy -->
    <owl:Class rdf:about="http://maison-elite.org/ontology#DegustationMenu">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Menu"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#ALaCarteMenu">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Menu"/>
    </owl:Class>
    <owl:Class rdf:about="http://maison-elite.org/ontology#SeasonalMenu">
        <rdfs:subClassOf rdf:resource="http://maison-elite.org/ontology#Menu"/>
    </owl:Class>

    <owl:Class rdf:about="http://maison-elite.org/ontology#Reservation"/>


